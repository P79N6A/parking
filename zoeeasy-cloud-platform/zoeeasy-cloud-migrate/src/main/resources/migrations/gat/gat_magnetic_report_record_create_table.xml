<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="gat_magnetic_report_record_create_table" author="lhj">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="gat_magnetic_report_record"/>
            </not>
        </preConditions>
        <createTable tableName="gat_magnetic_report_record" remarks="地磁检测记录">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_magnetic_report_record"/>
            </column>
            <column name="tenantId" type="bigint(20) unsigned" remarks="租户ID">
                <constraints nullable="false"/>
            </column>
            <column name="parkingId" type="bigint(20)" remarks="停车场ID"/>
            <column name="parkingLotId" type="bigint(20)" remarks="泊位ID"/>
            <column name="detectorId" type="bigint(20)" remarks="检测器ID">
                <constraints nullable="false"/>
            </column>
            <column name="provider" type="smallint(6)" remarks="地磁管理器类型(厂商) 数据字典"/>
            <column name="serialNumber" type="varchar(50)" remarks="(厂商)设备序列号"/>
            <column name="changeTime" type="DATETIME" remarks="变更时间，地磁检测到车位状态变更上传数据的时间"/>
            <column name="beginTime" type="DATETIME" remarks="入车时间"/>
            <column name="endTime" type="DATETIME" remarks="出车时间"/>
            <column name="changeType" type="tinyint(4)" remarks="泊位业务变更原因(1-车辆到达, 2-车辆离开)"/>
            <column name="occupyStatus" type="tinyint(4)" remarks="占用状态"/>
            <column name="creatorUserId" type="bigint(20)" remarks="创建者"/>
            <column name="creationTime" type="DATETIME" remarks="创建时间"/>
        </createTable>
        <createIndex tableName="gat_magnetic_report_record" indexName="idx_magnetic_tenantId">
            <column name="tenantId"/>
            <column name="parkingId"/>
            <column name="detectorId"/>
        </createIndex>
        <createIndex tableName="gat_magnetic_report_record" indexName="idx_magnetic_provider">
            <column name="provider"/>
            <column name="serialNumber"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>